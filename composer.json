{
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "carlcs/craft-redactorcustomstyles": "^4.0",
        "carlcs/craft-uielementfields": "^2.0",
        "craftcms/cms": "4.7.0",
        "craftcms/element-api": "3.0.1.1",
        "craftcms/redactor": "^3.0",
        "craftcms/webhooks": "^3.0",
        "doublesecretagency/craft-cpcss": "^2.6",
        "leo/craft-shopify": "dev-main",
        "leo/storefront": "^0.0.1",
        "nystudio107/craft-seomatic": "4.0.38",
        "sebastianlenz/linkfield": "^2.1",
        "spacecatninja/imager-x": "4.2.4",
        "spicyweb/craft-neo": "4.0.3",
        "vaersaagod/matrixmate": "^2.1",
        "verbb/super-table": "^3.0",
        "vlucas/phpdotenv": "^5.4.0"
    },
    "require-dev": {
        "craftcms/generator": "^1.6",
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "api\\": "api/",
            "utils\\": "utils/"
        }
    },
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0.2"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.craftcms.com",
            "canonical": false
        },
        {
            "type": "path",
            "url": "plugins/storefront"
        },
        {
            "type": "path",
            "url": "plugins/craft-shopify"
        }
    ]
}